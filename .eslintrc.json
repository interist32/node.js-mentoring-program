{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "no-console": 0
    }
}
